                    <property name="can-target">True</property>
                    <property name="text">0.01</property>
                    <property name="valign">center</property>
                    <property name="max-width-chars">20</property>
                    <property name="width-request">15</property>
                  </object>
                </child>
              </object>
            </child>"                        
          </object>
        </child>        

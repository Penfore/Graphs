                  <object class="GtkDropDown" id="import_separator">
                    <property name="valign">center</property>
                    <property name="model">
                      <object class="GtkStringList">
                        <items>
                          <item translatable="yes">.</item>
                          <item translatable="yes">,</item>
                        </items>
                      </object>
                    </property>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="AdwActionRow">
                <property name="visible">True</property>
                <property name="can-focus">True</property>
                <property name="title" translatable="yes">Column X</property>
                <property name="subtitle" translatable="yes">Which column to load for the X-data</property>
                <property name="use-underline">True</property>
